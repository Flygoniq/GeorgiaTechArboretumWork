            <li role="presentation">{{#link-to "home"}}Home{{/link-to}}</li>
            <li role="presentation">{{#link-to "tours"}}Tours{{/link-to}}</li>
            <li role="presentation" class="active">{{#link-to "trees"}}Trees{{/link-to}}</li>
