          </ul>
        </nav>
    </div>

    <div class="jumbotron">
        <h1>Tree List</h1>
    </div>

    
    <div>
        {{#each model as |tree|}}
            {{#link-to "treedetails" tree}}
                <a class="btn btn-info btn-bar" role="button">
                <img src="/assets/images/icons/Icon-HexTree.svg" height="50" width="50">
                {{tree.CommonName}}
                <p>{{tree.ScientificName}}</p>
                </a>
            {{/link-to}}
        {{else}}
            No Trees Found riperoo
        {{/each}}
    
    </div> <!-- /container -->
    
    
